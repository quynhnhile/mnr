model Container {
  id                   BigInt    @id @default(autoincrement())
  idTos                String?   @map("id_tos") @db.VarChar(200)
  idCont               String?   @map("id_cont") @db.VarChar(50)
  idContTos            String?   @map("id_cont_tos") @db.VarChar(200)
  vesselKey            String    @map("vessel_key") @db.VarChar(100)
  vesselImvoy          String?   @map("vessel_imvoy") @db.VarChar(20)
  vesselExvoy          String?   @map("vessel_exvoy") @db.VarChar(20)
  eta                  DateTime? @db.Timestamptz(3)
  etb                  DateTime? @db.Timestamptz(3)
  etd                  DateTime? @db.Timestamptz(3)
  bargeInKey           String?   @map("barge_in_key") @db.VarChar(100)
  bargeOutKey          String?   @map("barge_out_key") @db.VarChar(100)
  deliveryOrder        String?   @map("delivery_order") @db.VarChar(100)
  blNo                 String?   @map("bl_no") @db.VarChar(100)
  bookingNo            String?   @map("booking_no") @db.VarChar(100)
  houseBillNo          String?   @map("house_bill_no") @db.VarChar(100)
  containerNo          String    @map("container_no") @db.VarChar(20)
  classCode            String    @map("class_code") @db.VarChar(50)
  operationCode        String    @map("operation_code") @db.VarChar(50)
  fe                   String    @db.VarChar(10)
  containerStatusCode  String    @map("container_status_code") @db.VarChar(50)
  cargoTypeCode        String    @map("cargo_type_code") @db.VarChar(50)
  commodity            String?   @db.Text
  localSizeType        String    @map("local_size_type") @db.VarChar(10)
  isoSizeType          String    @map("iso_size_type") @db.VarChar(10)
  isLocalForeign       String    @map("is_local_foreign") @db.VarChar(5)
  jobModeCodeIn        String?   @map("job_mode_code_in") @db.VarChar(50)
  methodCodeIn         String?   @map("method_code_in") @db.VarChar(50)
  dateIn               DateTime? @map("date_in") @db.Timestamptz(3)
  jobModeCodeOut       String?   @map("job_mode_code_out") @db.VarChar(50)
  methodCodeOut        String?   @map("method_code_out") @db.VarChar(50)
  dateOut              DateTime? @map("date_out") @db.Timestamptz(3)
  requireSurveyDate    DateTime? @map("require_survey_date") @db.Timestamptz(3)
  repairMoveDate       DateTime? @map("repair_move_date") @db.Timestamptz(3)
  eirInNo              String?   @map("eir_in_no") @db.VarChar(50)
  eirOutNo             String?   @map("eir_out_no") @db.VarChar(50)
  stuffNo              String?   @map("stuff_no") @db.VarChar(50)
  unstuffNo            String?   @map("unstuff_no") @db.VarChar(50)
  serviceNo            String?   @map("service_no") @db.VarChar(50)
  draftNo              String?   @map("draft_no") @db.VarChar(50)
  invoiceNo            String?   @map("invoice_no") @db.VarChar(50)
  zoneCode             String?   @map("zone_code") @db.VarChar(50)
  yardCode             String?   @map("yard_code") @db.VarChar(50)
  lineCode             String?   @map("line_code") @db.VarChar(50)
  block                String?   @db.VarChar(5)
  bay                  String?   @db.VarChar(5)
  row                  String?   @db.VarChar(5)
  tier                 String?   @db.VarChar(5)
  area                 String?   @db.VarChar(5)
  vgm                  Boolean   @default(false) @db.Boolean
  mcWeight             Decimal?  @map("mc_weight") @db.Decimal(18, 2)
  tareWeight           Decimal?  @map("tare_weight") @db.Decimal(18, 2)
  maxGrossWeight       Decimal?  @map("max_gross_weight") @db.Decimal(18, 2)
  sealNo               String?   @map("seal_no") @db.VarChar(50)
  sealNo1              String?   @map("seal_no1") @db.VarChar(50)
  sealNo2              String?   @map("seal_no2") @db.VarChar(50)
  pol                  String?   @db.VarChar(5)
  pod                  String?   @db.VarChar(5)
  fpod                 String?   @db.VarChar(5)
  transitCode          String?   @map("transit_code") @db.VarChar(50)
  transitPort          String?   @map("transit_port") @db.VarChar(5)
  temperature          String?   @db.VarChar(10)
  vent                 Decimal?  @db.Decimal(10, 2)
  ventUnit             String?   @map("vent_unit") @db.VarChar(10)
  cusHold              Boolean   @default(false) @map("cus_hold") @db.Boolean
  terHold              Boolean   @default(false) @map("ter_hold") @db.Boolean
  terHoldReason        String?   @map("ter_hold_reason") @db.Text
  isSpecialWarning     Boolean   @default(false) @map("is_special_warning") @db.Boolean
  specialWarning       String?   @map("special_warning") @db.Text
  conditionCode        String?   @map("condition_code") @db.VarChar(50)
  classifyCode         String?   @map("classify_code") @db.VarChar(50)
  conditionMachineCode String?   @map("condition_machine_code") @db.VarChar(50)
  isTruckBarge         String?   @map("is_truck_barge") @db.VarChar(1)
  truckNo              String?   @map("truck_no") @db.VarChar(10)
  romoocNo             String?   @map("romooc_no") @db.VarChar(10)
  isBundled            Int?      @default(0) @map("is_bundled") @db.Integer // 0 : CONT THƯỜNG | 1 : CONT FLATRACK ĐÓNG BÓ | 2: CONT FLATRACK XẢ BÓ
  containerNoMaster    String?   @map("container_no_master") @db.VarChar(20)
  noteCont             String?   @map("note_cont") @db.Text
  noteDamage           String?   @map("note_damage") @db.Text
  noteSpecialHandling  String?   @map("note_special_handling") @db.Text
  note                 String?   @db.Text

  createdAt DateTime? @default(now()) @map("created_date") @db.Timestamptz(3)
  createdBy String    @map("created_by") @db.VarChar(36)
  updatedAt DateTime? @updatedAt @map("modified_date") @db.Timestamptz(3)
  updatedBy String?   @map("modified_by") @db.VarChar(36)

  @@map("dt_container")
}
