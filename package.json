  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:local": "cross-env NODE_ENV=local PORT=3000 nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:prod": "npx prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "test": "jest --config .jestrc.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest -i --config jest-e2e.json",
    "docker:env": "docker-compose --file docker/docker-compose.yml up --build",
    "module:generate": "nest g -c nest-hexa-schematics ddd-resource"
  },
